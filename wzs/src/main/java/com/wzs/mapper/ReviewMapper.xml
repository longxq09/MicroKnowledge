<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wzs.mapper.MGuessMapper" >

    <sql id="BASE_TABLE">
        review
    </sql>

    <sql id="BASE_COLUMN">
        `id`, `authorId`, `type`, `passNum`, `againstNum`, `topic`, `citation`, `keywords`, `summary`, `time`, `title`
    </sql>


    <select id="getReviewingMicroPubs" resultType="com.wzs.bean.Review">
        SELECT * FROM review
    </select>
</mapper>